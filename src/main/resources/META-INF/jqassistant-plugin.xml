<jqa-plugin:jqassistant-plugin
    xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1" name="Kieker">
    <description>Provides a scanner for Kieker files.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.MethodDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.TraceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.TypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.RecordDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.CallsDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.TypeDependsOnDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.CallEventDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.ExecutionEventDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.PackageDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.MeasurementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.CpuUtilizationMeasurementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.DiskUsageMeasurementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.LoadAverageMeasurementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.MemSwapUsageMeasurementDescriptor</class>
        <class>org.jqassistant.contrib.plugin.kieker.api.model.NetworkUtilizationMeasurementDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.kieker.impl.scanner.KiekerDirectoryScannerPlugin</class>
    </scanner>
    <rules>
        <resource>kieker.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
